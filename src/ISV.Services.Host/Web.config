<?xml version="1.0" encoding="utf-8"?>
<!--
  For more information on how to configure your ASP.NET application, please visit
  https://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1"/>
  </system.web>
  <system.serviceModel>
 
        <services>
            <service name="ISV.Services.FrontEnd" behaviorConfiguration="FrontEndBehavior">
                <endpoint contract="ISV.Services.Contracts.IFrontEnd" binding="basicHttpBinding" address="http://localhost:50745/" />
            </service>
        </services>
    
        <bindings>  
          <basicHttpBinding>  
            <binding> 
               <security mode="None">  
                   <transport clientCredentialType="Windows" />  
               </security>  
               <readerQuotas />  
            </binding>  
          </basicHttpBinding> 
        </bindings> 
    
        <behaviors>
            <serviceBehaviors>
                <behavior name="FrontEndBehavior" >
                    <serviceMetadata httpGetEnabled="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
 
    </system.serviceModel>
</configuration>